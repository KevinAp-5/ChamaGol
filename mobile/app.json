{
  "expo": {
    "expo.doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    },
    "scheme": "chamagol",
    "name": "ChamaGol",
    "slug": "ChamaGol",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/splash.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kevenap5.ChamaGol",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "splash": {
        "image": "./src/assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kevenap5.ChamaGol",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "chamagol"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "splash": {
        "image": "./src/assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000",
        "mdpi": "./src/assets/splash.png",
        "hdpi": "./src/assets/splash.png",
        "xhdpi": "./src/assets/splash.png",
        "xxhdpi": "./src/assets/splash.png",
        "xxxhdpi": "./src/assets/splash.png"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "splash": {
        "image": "./src/assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "extra": {
      "eas": {
        "projectId": "2a5d08e5-afdc-4785-befc-b54adb5a9941"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "owner": "kevenap-5"
  }
}